{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [
          "> 1%",
          "last 7 versions",
          "last 10 Chrome versions",
          "last 5 Firefox versions",
          "Safari >= 6",
          "ie > 8"
        ]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-runtime",
		["import", {
			"libraryName": "vant",
			"libraryDirectory": "es",
			"style": true
		}]
	]
}
